{
  "extends": ["config:base"],
  "npm": {
    "fileMatch": ["(^|/)deps.json$"]
  },
  "postUpgradeTasks": {
    "commands": ["node update-deps-in-template.js"],
    "fileFilters": ["template.json"]
  },
  "automerge": true,
  "major": {
    "automerge": true
  },
  "minor": {
    "automerge": true
  },
  "prHourlyLimit": 1,
  "updateNotScheduled": false,
  "timezone": "Australia/Sydney",
  "lockFileMaintenance": {
    "enabled": true
  },
  "masterIssue": true
}